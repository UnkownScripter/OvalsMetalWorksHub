DupeScript.MouseButton1Click:Connect(function()
	pop:Play()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/UnkownScripter/MetalworksDupe/main/Script', true))()
end)

afkscrap.MouseButton1Click:Connect(function()
	pop:Play()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/UnkownScripter/OvalsMetalWorksHub/main/afkdupescript', true))()
end)

mwstoolkit.MouseButton1Click:Connect(function()
	pop:Play()
	loadstring(game:HttpGet('https://pastebin.com/raw/h9Fm8KVZ', true))()
end)

Close.MouseButton1Click:Connect(function()
	bye:Play()
	main.Visible = false
	wait(3)
	OvalsMetalWorksHub:Destroy()
end)

OvalsUtilities.MouseButton1Click:Connect(function()
	pop:Play()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/UnkownScripter/OvalsMetalWorksHub/main/Utilities', true))()
end)
